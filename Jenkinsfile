pipeline{
    agent any
    stages{
        stage('Build'){
            steps{
                echo "This is a build stage"
            }
        }
        stage('Test'){
            steps{
                echo "This is a test stage"
            }
        }
        stage('Deploy'){
            steps{
                echo "This is a deploy stage"
            }
        }
        stage('Monitor'){
            steps{
                echo "This is a monitor stage"
            }
        }
    }
}